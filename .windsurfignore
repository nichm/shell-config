node_modules/
.pnpm-store
bower_components/

# Build outputs (no need to index generated files)
dist/
build/
out/
.next/
.cache/
.parcel-cache/

# Environment files (security)
.env*
!.env.example
!.env.template

# Logs and temporary data
logs/
*.log
tmp/
temp/

# Testing and coverage
coverage/
*.lcov
.nyc_output/

# Binary files (can't analyze in text)
*.png
*.jpg
*.jpeg
*.gif
*.ico
*.svg
